// Exercício 06 - Write a program to print full name of a from first name and last name using user input.

import 'dart:io';

void main() {
  print('Informe seu primeiro nome: ');
  String nome1 = stdin.readLineSync()!;
  
  print('Informe seu segundo nome: ');
  String nome2 = stdin.readLineSync()!;
  
  print('Seu nome completo é: $nome1 $nome2');
}
